{
  "name": "rc-datetime-picker",
  "version": "1.2.0",
  "description": "React datetime picker by momentjs",
  "main": "dist/rc-datetime-picker.cjs.js",
  "keywords": [
    "react",
    "date",
    "datetime",
    "picker",
    "moment"
  ],
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --progress --colors --content-base ./docs --ENV=development",
    "build-css": "lessc --clean-css src/less/picker.less dist/picker.min.css && lessc src/less/picker.less dist/picker.css",
    "build-js": "node build/build.js",
    "build": "npm run build-css && npm run build-js",
    "test": "jest",
    "release": "bash build/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllenWooooo/rc-datetime-picker.git"
  },
  "author": "Allen Wu <mangong33@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AllenWooooo/rc-datetime-picker/issues"
  },
  "homepage": "https://github.com/AllenWooooo/rc-datetime-picker#readme",
  "dependencies": {
    "blacklist": "^1.1.4",
    "classnames": "^2.2.5",
    "moment": "^2.17.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-slider": "^0.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-external-helpers": "^6.18.0",
